{
  "name": "@nurv-llc/redact-it",
  "version": "1.0.3",
  "description": "A TypeScript library for redacting sensitive information from objects by matching property names",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "dev": "npm run build:watch"
  },
  "keywords": [
    "redact",
    "redaction",
    "privacy",
    "data-sanitization",
    "typescript",
    "security",
    "pii",
    "sensitive-data"
  ],
  "author": "NURV LLC",
  "license": "MIT",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cyb3rsyph0n/redact-it.git"
  },
  "bugs": {
    "url": "https://github.com/cyb3rsyph0n/redact-it/issues"
  },
  "homepage": "https://github.com/cyb3rsyph0n/redact-it#readme",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
